   ],
   "source": [
    "def factorial(n):\n",
    "#     base case\n",
    "    if n == 1:\n",
    "        return (1)\n",
    "    else:\n",
    "        return(n*factorial(n-1))\n",
    "    \n",
    "factorial(10)"
   ]
  }
