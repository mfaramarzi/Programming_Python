    "def primality(n):\n",
    "    \n",
    "    if n < 2:\n",
    "        \n",
    "        return(\"Not prime\")\n",
    "        \n",
    "    for i in range (2 , math.ceil(n/2)):\n",
    "        \n",
    "        if (n % i == 0):\n",
    "            \n",
    "            return(\"Not prime\")\n",
    "        \n",
    "    return(\"Prime\")"
   ]
